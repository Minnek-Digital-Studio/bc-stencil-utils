{
    "parser": "@babel/eslint-parser",
    "env": {
        "es6": true,
        "browser": true,
        "jest": true
    },
    "extends": ["airbnb-base", "eslint-config-prettier"],
    "plugins": ["eslint-plugin-prettier"],
    "ignorePatterns": ["dist"],
    "rules": {
        "indent": [
            2,
            4
        ],
        "no-alert": 0,
        "consistent-return": 0,
        "max-len": 0,
        "prefer-rest-params": 0,
        "class-methods-use-this": 0
    }
}
